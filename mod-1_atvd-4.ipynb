   ],
   "source": [
    "valor = 9\n",
    "cont = 1\n",
    "\n",
    "while cont <= 10:\n",
    "    print('{} x {} = {}' .format(valor, cont, valor*cont) )\n",
    "    cont = cont + 1"
   ]
